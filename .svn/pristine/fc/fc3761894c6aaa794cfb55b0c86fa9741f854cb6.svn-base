using System;

namespace MISL.Ababil.Agent.Infrastructure.Models.models.transaction
{


    [Serializable]
    public class TransactionRequest
    {
        /// 
        private const long serialVersionUID = 1L;
        private long _referenceNumber;
        private string _userName;
        private string _terminal;
        private decimal _amount;
        private long _transactionDate;
        private decimal _chargeAmount;



        public virtual long transactionDate
        {
            get
            {
                return _transactionDate;
            }
            set
            {
                this._transactionDate = value;
            }
        }
        public virtual long referenceNumber
        {
            get
            {
                return _referenceNumber;
            }
            set
            {
                this._referenceNumber = value;
            }
        }
        public virtual string userName
        {
            get
            {
                return _userName;
            }
            set
            {
                this._userName = value;
            }
        }
        public virtual string terminal
        {
            get
            {
                return _terminal;
            }
            set
            {
                this._terminal = value;
            }
        }
        public virtual decimal amount
        {
            get
            {
                return _amount;
            }
            set
            {
                this._amount = value;
            }
        }

        public virtual decimal chargeAmount
        {
            get
            {
                return _chargeAmount;
            }
            set
            {
                this._chargeAmount = value;
            }
        }
    }

}
